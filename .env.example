# Vector Database Configuration

# Storage type: "memory" (default) or "disk"
STORAGE_TYPE=memory

# Data directory for disk storage (only used when STORAGE_TYPE=disk)
DATA_DIR=./data

# Server configuration
HOST=0.0.0.0
PORT=8000

# Logging configuration
# LOG_LEVEL options: DEBUG, INFO (default), WARNING, ERROR, CRITICAL
LOG_LEVEL=INFO
LOG_FORMAT=%(asctime)s - %(name)s - %(levelname)s - %(message)s
